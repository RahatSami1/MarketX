      "package": "com.abhishekgurjar.marketX"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "726d935b-65aa-43f5-aad9-e8c1b2275bcf"
